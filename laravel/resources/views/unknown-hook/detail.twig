{% extends "../basic.twig" %}

{% block title %}unknown hook detail{% endblock %}
{% block head %}
    <style>
        dd{margin-left:20px;
            word-wrap: break-word}
    </style>
{% endblock %}

{% block content %}
    <a href="{{ route('unknown.hooks') }}">back</a>
    <h1>unknown hook detail</h1>
    <p> Use this to
        <strong><a href="{{ route('hook.create-page',[unknownHook.id]) }}">Generate Hook</a></strong>
    </p>
    {% for blockKey,block in unknownHook.parseRequest() %}
        <h3>{{ blockKey }}</h3>
        {% for key,value in block %}
            <dl>
                <dt>{{ key }}</dt>
                <dd>{{ value }}</dd>
            </dl>
        {% else %}
            No data here.
        {% endfor %}
    {% endfor %}

{% endblock %}